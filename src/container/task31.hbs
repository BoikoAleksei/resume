<div>
    <section>
        {{#with navigation}}
        {{#each links}}
      <a href={{"href"}} class="btn btn-link">{{text}}</a>
        {{/each}}
        {{#with button}}
              <a href={{"href"}} class="btn btn-primary">{{text}}</a>
        {{/with}}
        {{/with}}
    </section>    
</div>    
<hr>
<div>
    <section>
{{#with header}}
<h1>{{title}}</h1>
<p>{{description}}</p>
{{#with button}}
    <a href={{"link"}} class="btn btn-primary">{{text}}</a>
{{/with}}
{{/with}}
    </section>
</div>
<hr>
<div>
    <section>
        {{#with city}}
        <h2>{{title}}</h2>
        <div class="row gx-5 gy-1">
        {{#each cards}}
{{#with picture}}
<section class="col-5">
<div class="card text">
    <img class="card-img-top" src="http://picsum.photos/400/200" alt="Product photo"> 
    <div class="card-body">
        <h5 class="card-title text-bg-primary">{{title}}</h5>
{{/with}}

      <p class="text">{{description}}</p>

      {{#with button}}
      <a href={{""}} class="btn btn-primary"> {{text}} </a>
      {{/with}}

    </div>
</div>
</section>
{{/each}}
</div>
{{/with}}
    </section>
</div>
<br>
<hr>
<div>
    <section>
        {{#with winner}}
        <h2>{{title}}</h2>
        {{#with table}}
        <table class="table table-striped table-bordered">
    <thead>
        <tr>
             <th>
                #
            </th>
            <th>
                name
            </th>
             <th>
                surname
            </th>
            <th>
                nickname
            </th>
        </tr>
    </thead>
     <tbody>
        {{#each user}}
        <tr>
            <th>{{place}}</th>
            <td>{{name}}</td>
            <td>{{surname}}</td>
            <td>{{nickname}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>
{{/with}}
{{/with}}
    </section>
</div>
<br>
<hr>
<br>
<div>
<section>
        {{#with gallery}}
        <h2>{{title}}</h2>
       <div class="row gx-5 gy-4">
        {{#each cards}}
<section class="col-4">
<div class="card text">
        <h5 class="card-header">{{name}}</h5>
      <div class="list-group"> 
    <div class="list-group-item">{{items}}</div>
    </div>
    </div>
</section>
{{/each}}
</div>
{{/with}}
    </section>
</div>
<br>
<hr>
<br>
<div>
    <section>
        {{#with relaxion}}
        
<div class="display-3">{{title}}</div>
<div class="row gx-5 gy-1">
{{#each cards}}
    <div class="col-5">
      <div class="card text-bg-secondary">
    <div class="card-header text-center">{{header}}</div>
    <div class="card-body text-center">
        <h6> <div class= "card-title">{{title}}</div> </h6>
        <p>{{description}}</p>
        </div>
        </div>
        </div>
{{/each}}
</div>
{{/with}}
</section>
</div>

<br>
<hr>
<br>
<div>
    <section>
        {{#with rest}}
        <div class="row gx-5 gy-1">
        {{#each cards}}
        
    <div class="col-5">
      <div class="card text">
    <div class="card-header text-center">{{header}}</div>
    <div class="card-body text-center">
        <h6> <div class= "card-title">{{title}}</div> </h6>
        <p>{{description}}</p>
        <button class="btn btn-primary">{{button}}</button>
</div>
<div class="card-footer text-center">{{footer}}</div>
</div>
</div> 
{{/each}}
</div>
{{/with}}
    </section>
</div>
<br>
<hr>
<br>
<div>
    <section>
        {{#with proposition}}
        <div class="display-2">{{title}}</div>
    </section>
    <div>
        <div class="row gx-5 gy-1">
{{#each card}}
    <div class="col-5">
      <div class="card text-bg-warning">
    <div class="card-header text-light">{{header}}</div>
    <div class="card-body">
        <h6> <div class= "card-title text-light">{{title}}</div> </h6>
        <p class="text-light">{{description}}</p>
        </div>
        </div>
        </div>
{{/each}}
</div>
{{/with}}
    </section>
</div>
<br>
<hr>
<br>
<div>
        {{#with footer}}
        <h5>{{title}}</h5> 

    <div class="row">
        <div class="col-3">
        {{#each navigation}}
        <div>
    <a class="btn btn-outline-secondary text-info"href={{"href"}}>{{text}}</a>
       </div>
        {{/each}}
        </div>

<div class="col-3">
        {{#each socialMedia}}
        <div>
    <a class="btn btn-outline-dark text-info"href={{"link"}}>{{name}}</a>
       </div>
        {{/each}}
        </div>

        <div class="col-3">
        {{#each partners}}
        <div>
    <a class="btn btn-outline-info text-info"href={{"likn"}}>{{name}}</a>
       </div>
        {{/each}}
        </div>        
{{/with}}
</div>











    

    
 






