{{!-- 
Це код container файлу для summary сторінки, 
в якому міститься код, котрий буде віддаватися по шляху (PATH) - "/summary"
--}}

<section>

<h1>{{name}}</h1>
<hr>
 {{description}}
<br>
 {{#with history}}
 {{year}}
 <br>
    {{founder}}
    {{/with}}

{{#each types}}
<li>
{{this}}
</li>
{{/each}}

<div>
{{#with syntax}}
<br>
{{variables}}
<br>
{{functions}}
<br>
{{conditionals}}
<br>
{{loops}}
<br>
{{classes}}
{{/with}}

{{#each features}}
<li>
{{this}}
</li>
{{/each}}
<br>
{{#each libraries}}
<li>
{{this}}
</li>
{{/each}}
<br>
{{#each tools}}
<li>
{{this}}
</li>
{{/each}}
<br>

{{#each community}}
<li>
{{name}}
<br>
{{type}}
</li>
{{/each}}
</div>
</section>







</section>









    

    
 






